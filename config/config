export PLUGIN_WORKSPACE_CONFIGS=$(plugin::get_app_configs_path $PLUGIN_WORKSPACE_NAME)/config

export PLUGIN_WORKSPACE_CODE=$HOME/code

export PLUGIN_WORKSPACE_REMOTEDEV_IMAGE_NAME=remotedev
export PLUGIN_WORKSPACE_REMOTEDEV_IMAGE_VERSION=v1.3.7

